{
    "window.zoomLevel": 0.8,
    "security.workspace.trust.enabled": false,
    "tabout.disableByDefault": false,
    "git.openRepositoryInParentFolders": "always",
    "dotnetAcquisitionExtension.existingDotnetPath": [
        {
            "extensionId": "msazurermtools.azurerm-vscode-tools",
            "path": "C:\\Program Files\\dotnet\\dotnet.exe"
        }
    ],
    "editor.tabCompletion": "on",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.trimAutoWhitespace": true,
    "editor.renderWhitespace": "all",
    "editor.snippetSuggestions": "inline",
    "editor.fontFamily": "'Fira Code Retina', '빙그레 따옴체'",
    "editor.wordWrap": "on",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,

    "editor.autoClosingQuotes": "languageDefined",
    "editor.suggestFontSize": 11,
    "editor.suggest.selectionMode": "always",
    "editor.smoothScrolling": true,
    "editor.minimap.enabled": false,
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "expand",
    "editor.renderControlCharacters": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.markdownlint": true
    },
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": "off"
    },
    "terminal.integrated.fontFamily": "'FiraCode Nerd Font', '빙그레 따옴체'",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.tabs.hideCondition": "singleTerminal",
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.letterSpacing": 1.2,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.profiles.windows": {
        "Default PowerShell": {
            "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
            "args": [
                "-NoLogo"
            ]
        },
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "args": [
                "-NoLogo"
            ]
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "terminal.integrated.defaultProfile.windows": "Default PowerShell",
    "debug.console.fontSize": 12,
    "debug.console.fontFamily": "'FiraCode Nerd Font', '빙그레 따옴체'",
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.encoding": "utf8",
    "[powershell]": {
        "files.encoding": "utf8bom",
        "files.autoGuessEncoding": true,
        "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
        "editor.semanticHighlighting.enabled": false,
        "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
    },
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "markdown-mermaid.languages": [
        "mermaid"
    ],
    "[csharp]": {
        "editor.maxTokenizationLineLength": 2500
    },
    "markdown-mermaid.lightModeTheme": "forest",
    "markdown.extension.math.enabled": true,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.autoClosingQuotes": "never",
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true
    },
    "http.proxyAuthorization": null,
    "python.defaultInterpreterPath": "C:\\Program Files\\Python311\\python.exe",
    "redhat.telemetry.enabled": true,
